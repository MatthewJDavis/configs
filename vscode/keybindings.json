// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "ctrl+shift+p",
        "command": "git.push"
    },
    {
        "key": "ctrl+oem_8",
        "command": "workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "ctrl+shift+g g",
        "command": "workbench.view.scm",
        "when": "gitlens:enabled && gitlens:keymap == 'chorded'"
    },
    {
        "key": "ctrl+alt+g g",
        "command": "-workbench.view.scm",
        "when": "gitlens:enabled && gitlens:keymap == 'chorded'"
    }
]